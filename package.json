    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-babel": "5.1.0",
    "jest": "23.4.2",
    "jest-cli": "23.4.2",
    "jest-environment-node-debug": "2.0.0",
    "prettier": "1.14.0",
    "regenerator-runtime": "0.12.0",
    "semantic-release": "15.9.5"
  },
  "dependencies": {
    "@bitdiver/definition": "1.0.2",
